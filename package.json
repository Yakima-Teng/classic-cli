{
    "name": "classic-cli",
    "version": "1.0.3",
    "description": "一个快速创建H5专题页的命令行工具，方便使用者使用构建工具进行无侵入式开发（可随时脱离构建工具）。",
    "main": "bin/classic",
    "bin": {
        "classic": "bin/classic"
    },
    "scripts": {
        "classic": "node bin/classic",
        "test": "npm run classic -- -p 8080 -t remote"
    },
    "files": [
        "bin",
        "lib",
        "LICENSE",
        "package.json",
        "package-lock.json",
        "README.md"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Yakima-Teng/classic-cli.git"
    },
    "keywords": [
        "less"
    ],
    "author": "Yakima (yakima.public@gmail.com)",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Yakima-Teng/classic-cli/issues"
    },
    "homepage": "https://github.com/Yakima-Teng/classic-cli#readme",
    "dependencies": {
        "@babel/core": "^7.9.6",
        "@babel/preset-env": "^7.9.6",
        "chalk": "^4.0.0",
        "commander": "^5.1.0",
        "download-git-repo": "^3.0.2",
        "fs-extra": "^8.1.0",
        "glob": "^7.1.6",
        "gulp": "^4.0.2",
        "gulp-autoprefixer": "^7.0.1",
        "gulp-babel": "^8.0.0",
        "gulp-beautify": "^3.0.0",
        "gulp-clean-css": "^4.3.0",
        "gulp-connect": "^5.7.0",
        "gulp-if": "^3.0.0",
        "gulp-uglify": "^3.0.2",
        "ora": "^4.0.4"
    },
    "engines": {
        "node": ">=9"
    },
    "browserslist": {
        "production": [
            ">0.01%",
            "last 20 version"
        ],
        "development": [
            ">0.01%",
            "last 20 version"
        ]
    }
}
